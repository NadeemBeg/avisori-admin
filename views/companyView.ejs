  <script>
    $(function () {
      $("#example1").DataTable({
        "responsive": true, "lengthChange": false, "autoWidth": false,
        "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"]
      }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
      $('#example2').DataTable({
        "paging": true,
        "lengthChange": false,
        "searching": false,
        "ordering": true,
        "info": true,
        "autoWidth": false,
        "responsive": true,
      });
    });
    function validateform(){  
      var email=document.myform.email.value;  
      var phoneNumber=document.myform.phoneNumber.value;  
      var organisationNumber=document.myform.organisationNumber.value; 
      if (email == null || email == ""){  
        alert("Email can't be blank");  
        return false;  
      }
      else if(phoneNumber.length < 10 || phoneNumber.length > 10){  
        alert("phoneNumber is not valid");  
        return false;  
        }   
      else if (organisationNumber==null || organisationNumber==""){  
        alert("Organisation Number can't be blank");  
        
        return false;  
      }
    }
